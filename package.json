{
  "name": "tree-sitter-kalix",
  "version": "0.1.0",
  "description": "Tree-sitter grammar for Kalix smart contract language",
  "main": "bindings/node",
  "keywords": [
    "tree-sitter",
    "parser",
    "kalix",
    "smart-contracts",
    "blockchain",
    "hedera",
    "zvm"
  ],
  "author": "ghostkellz",
  "license": "MIT",
  "dependencies": {
    "node-addon-api": "^8.0.0",
    "node-gyp-build": "^4.8.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.25.0",
    "prebuildify": "^6.0.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "build": "tree-sitter generate && node-gyp-build",
    "test": "tree-sitter test",
    "parse": "tree-sitter parse"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ghostkellz/tree-sitter-kalix"
  },
  "tree-sitter": [
    {
      "scope": "source.kalix",
      "file-types": [
        "kalix"
      ],
      "injection-regex": "kalix",
      "highlights": [
        "queries/highlights.scm"
      ],
      "locals": [
        "queries/locals.scm"
      ]
    }
  ]
}
